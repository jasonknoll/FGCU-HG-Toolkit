<MainMenu>:
    Screen:
        GridLayout:
            cols: 1    
            Label:
                id: test_login_label 
                text: ""
                markup: True

            Button:
                id: test_login_button
                text: "Login"
                on_press: 
                    root.login()


            Button:
                id: test_graph_menu_button
                text: "Graphing"
                on_press: 
                    root.sm.current = 'graph'

